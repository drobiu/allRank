{
  "model": {
    "fc_model": {
      "sizes": [96],
      "input_norm": false,
      "activation": "Sigmoid",
      "dropout": 0.1
    },
    "transformer": false,
    "post_model": {
      "output_activation": "Tanh",
      "d_output": 1
    }
  },
  "data": {
    "path": "./data/MQ2008",
    "validation_ds_role": "vali",
    "num_workers": 1,
    "batch_size": 64,
    "slate_length": 240
  },
  "optimizer": {
    "name": "Adam",
    "args": {
      "lr": 0.001
    }
  },
  "lr_scheduler": {
    "name": "StepLR",
    "args": {
      "step_size": 50,
      "gamma": 0.1
    }
  },
  "training": {
    "epochs": 100,
    "early_stopping_patience": 10,
    "gradient_clipping_norm": null
  },
  "val_metric": "ndcg_5",
  "metrics": [
    "mrr",
    "mrr2",
    "map",
    "map2",
    "ndcg",
    "ndcg_5",
    "ndcg_10",
    "precision_1",
    "precision_5",
    "precision_10",
    "precision2_1",
    "precision2_5",
    "precision2_10",
    "recall_1",
    "recall_5",
    "recall_10",
    "recall2_1",
    "recall2_5",
    "recall2_10",
    "rmse2"

  ],
  "loss": {
    "name": "neuralNDCG",
    "args": {
      "temperature": 1.0,
      "k": null,
      "powered_relevancies": true,
      "stochastic": false
    }
  },
  "detect_anomaly": false,
  "expected_metrics" : {
    "val": {
      "ndcg_5": 0.5
    }
  }
}